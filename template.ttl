@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix doap: <http://usefulinc.com/ns/doap#> .

<https://dfdx.eu/fps-plugins.lv2/fixed-ir-COLLECTION.lv2> a lv2:Plugin ;
  doap:name "Relative dynamics";
  doap:license <http://usefulinc.com/doap/licenses/gpl> ;
  doap:description "IRs: COLLECTION" ;
  lv2:binary <fixed-ir-COLLECTION.so> ;
  lv2:optionalFeature lv2:hardRTCapable ;
  lv2:port [
    a lv2:AudioPort ;
    a lv2:InputPort ;
    lv2:index 0 ;
    lv2:symbol "in" ;
    lv2:name "In"
  ] , [
    a lv2:AudioPort ;
    a lv2:OutputPort ;
    lv2:index 1 ;
    lv2:symbol "out" ;
    lv2:name "Out"
  ] , [
    a lv2:InputPort ;
    a lv2:ControlPort ;
    lv2:index 2 ;
    lv2:symbol "gain" ;
    lv2:name "Gain";
    lv2:default 0 ;
    lv2:minimum -32 ;
    lv2:maximum 32 ;
  ]  , [
    a lv2:InputPort ;
    a lv2:ControlPort ;
    lv2:index 3 ;
    lv2:symbol "drywet" ;
    lv2:name "Dry/Wet";
    lv2:default 1 ;
    lv2:minimum 0 ;
    lv2:maximum 1 ;
  ]  , [
    a lv2:InputPort ;
    a lv2:ControlPort ;
    lv2:index 4 ;
    lv2:symbol "ir" ;
    lv2:name "IR";
    lv2:default 0 ;
    lv2:minimum 0 ;
    lv2:maximum NUMBER_OF_IRS ;
  ] .
